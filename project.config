[project]
	description = The Go driver for MongoDB
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Kris Brandow (skriptble)
[access "refs/for/refs/*"]
	read = group user/Kris Brandow (skriptble)
	push = group user/Kris Brandow (skriptble)
	label-Code-Review = -2..+2 group user/Kris Brandow (skriptble)
	label-Verified = -1..+1 group user/Kris Brandow (skriptble)
	rebase = group user/Kris Brandow (skriptble)
	removeReviewer = group user/Kris Brandow (skriptble)
	submit = group user/Kris Brandow (skriptble)
[access "refs/heads/*"]
	read = group user/Kris Brandow (skriptble)
	create = group user/Kris Brandow (skriptble)
	push = +force group user/Kris Brandow (skriptble)
	pushMerge = group user/Kris Brandow (skriptble)
[access "refs/tags/*"]
	push = group user/Kris Brandow (skriptble)
	push = +force group user/Kris Brandow (skriptble)
